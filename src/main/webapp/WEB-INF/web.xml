<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registroServlet</servlet-name>
        <servlet-class>Servlets.registroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>accesoServlet</servlet-name>
        <servlet-class>Servlets.accesoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>eliminarServlet</servlet-name>
        <servlet-class>Servlets.eliminarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateServlet</servlet-name>
        <servlet-class>Servlets.updateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mostrarServlet</servlet-name>
        <servlet-class>Servlets.mostrarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registroServlet</servlet-name>
        <url-pattern>/registroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>accesoServlet</servlet-name>
        <url-pattern>/accesoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>eliminarServlet</servlet-name>
        <url-pattern>/eliminarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateServlet</servlet-name>
        <url-pattern>/updateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mostrarServlet</servlet-name>
        <url-pattern>/mostrarServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
